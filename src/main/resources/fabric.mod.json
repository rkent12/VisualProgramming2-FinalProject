{
  "schemaVersion": 1,
  "id": "practicemod",
  "version": "${version}",

  "name": "Example Mod",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "Ryan Kent"
  ],
  "contact": {
    "homepage": "https://rkent@mail.endicott.edu/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/practicemod/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.ryankent.practicemod.PracticeMod"
    ]
  },
  "entrypoints": {
    "client": [
      "net.ryankent.practicemod.client.ModClient"
    ]
  },
  "mixins": [
    "practicemod.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.17.x",
    "java": ">=16"
  },
  "suggests": {
    "another-mod": "*"
  }
}

